    #   --> 1
    # Requires: ! CYGSEM_HAL_MICROBLAZE_COPY_VECTORS
    #     CYGSEM_HAL_MICROBLAZE_COPY_VECTORS == 1
    #   --> 0
    # Requires: ! CYGDBG_HAL_DEBUG_GDB_INCLUDE_STUBS
